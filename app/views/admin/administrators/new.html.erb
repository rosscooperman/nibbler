<% @page_title = "Create an admin" %>

<% form_for [:admin, @administrator] do |f| %>
  <%= render :partial => "form", :locals => { :f => f } %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>
  <%= f.submit "Create it!" %>
<% end %>